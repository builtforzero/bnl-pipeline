l":{"line":215,"column":8},"generated":{"line":1,"column":2165}},{"source":"style.css","name":null,"original":{"line":215,"column":34},"generated":{"line":1,"column":2190}},{"source":"style.css","name":null,"original":{"line":216,"column":8},"generated":{"line":1,"column":2191}},{"source":"style.css","name":null,"original":{"line":216,"column":24},"generated":{"line":1,"column":2206}},{"source":"style.css","name":null,"original":{"line":217,"column":8},"generated":{"line":1,"column":2207}},{"source":"style.css","name":null,"original":{"line":217,"column":21},"generated":{"line":1,"column":2219}},{"source":"style.css","name":null,"original":{"line":218,"column":8},"generated":{"line":1,"column":2220}},{"source":"style.css","name":null,"original":{"line":219,"column":4},"generated":{"line":1,"column":2230}},{"source":"style.css","name":null,"original":{"line":220,"column":0},"generated":{"line":1,"column":2231}}],"sources":{"style.css":":root {\n    /* Colors */\n    --color-main: #a50a51;\n    --color-second: #0494cb;\n    --color-neutral: #636467;\n    --color-alert: rgb(235, 51, 51);\n\n    /* Fonts */\n    --font-title: 'Asap Condensed', sans-serif;\n    --font-body: 'Raleway', sans-serif;\n\n    /* Font Sizes */\n    --root-size: 16px;\n    --h1-size: 3.2em;\n    --h2-size: 2.2em;\n    --h3-size: 1.3em;\n    --h4-size: 1.1em;\n    --subtitle-size: 0.8em;\n    --tiny-size: 0.6em;\n}\n\nhtml,\nbody,\nh1,\nh2,\nh3,\np {\n    margin: 0;\n    padding: 0;\n}\n\nhtml {\n    font-size: var(--root-size);\n    font-family: var(--font-body);\n    background-color: lightgrey;\n}\n\nbody {\n    font-family: var(--font-body);\n}\n\nh1 {\n    font-family: var(--font-title);\n    font-size: var(--h1-size);\n}\n\nh2 {\n    font-family: var(--font-title);\n    font-size: var(--h2-size);\n}\n\n.list {\n    column-count: 2;\n    margin: 15px 0px;\n}\n\n.center {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n\nbutton {\n    padding: 10px;\n    margin: 5% 0%;\n    width: 50%;\n    font-family: var(--font-body);\n    font-size: var(--h3-size);\n    font-weight: 900;\n    background-color: var(--color-main);\n    color: white;\n    border: 3px solid var(--color-main);\n}\n\nbutton:hover {\n    font-family: var(--font-body);\n    font-size: var(--h3-size);\n    font-weight: 900;\n    background-color: white;\n    color: var(--color-main);\n    border: 3px solid var(--color-main);\n    cursor: pointer;\n}\n\n.hide {\n    position: absolute !important;\n    top: -9999px !important;\n    left: -9999px !important;\n}\n\n\n/* COLUMNS */\n\n.columns {\n    display: grid;\n    grid-template-columns: 1fr 2fr;\n    column-gap: 10px;\n    margin: 10px;\n}\n\n.status {\n    border: 1px solid grey;\n    margin: 1%;\n    padding: 5%;\n}\n\n.step-header {\n    padding: 20px;\n    margin: 0%;\n    line-height: 200%;\n    background-color: var(--color-main);\n    color: white;\n}\n\n.step-content {\n    padding: 4%;\n    margin: 0%;\n    background-color: white;\n}\n\n/* FORM FIELDS */\n\ninput,\nselect {\n    font-family: var(--font-body);\n    font-size: 1rem;\n    margin: 5px 0px;\n    padding: 10px;\n    width: 95.5%;\n    font-weight: 500;\n}\n\nselect {\n    width: 100%;\n}\n\n.form-field {\n    padding: 15px 10px;\n    background-color: white;\n    font-weight: 600;\n}\n\n/* SUCCESS / FAIL */\n\n.success {\n    color: var(--color-second);\n}\n\n.fail {\n    color: var(--color-alert);\n}\n\n.neutral {\n    color: grey;\n}\n\n\n/* VALIDATION STEPS */\n\n.validation-step {\n    display: grid;\n    grid-template-columns: 5fr 1fr;\n    column-gap: 10px;\n    border-bottom: 1px solid lightgrey;\n}\n\n.validation-name {\n    font-size: var(--h3-size);\n    font-weight: 500;\n    margin: 0%;\n    padding: 15px 10px;\n}\n\n.validation-name:hover {\n    background-color: rgb(233, 232, 232);\n    cursor: pointer;\n}\n\n.validation-info {\n    margin: 0%;\n    padding: 2%;\n}\n\n.validation-symbol {\n    font-size: var(--h3-size);\n    font-weight: 900;\n}\n\n.toggle {\n    font-size: var(--tiny-size);\n    padding: 10px 0px 0px 0px;\n    color: var(--color-main);\n    font-weight: 600;\n}\n\n.info {\n    background-color: rgb(231, 231, 231);\n    color: rgb(51, 51, 51);\n    padding: 10px;\n    margin: 0px 0px 10px 0px;\n}\n\n.result {\n    padding: 10px;\n    margin: 10px 0px;\n}\n\n\n/* MEDIA BREAKPOINTS */\n\n/* Smartphones (portrait) ----------- */\n@media only screen and (max-width : 800px) {\n    .columns {\n        display: grid;\n        grid-template-columns: 1fr;\n        column-gap: 10px;\n        row-gap: 20px;\n        margin: 5px;\n    }\n}"},"lineCount":null}},"error":null,"hash":"3a8f5669844a823987dce5f538d51307","cacheData":{"env":{}}}