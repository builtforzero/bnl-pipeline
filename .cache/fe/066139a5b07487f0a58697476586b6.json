inal":{"line":303,"column":21},"generated":{"line":1,"column":3234}},{"source":"style.css","name":null,"original":{"line":304,"column":8},"generated":{"line":1,"column":3235}},{"source":"style.css","name":null,"original":{"line":304,"column":34},"generated":{"line":1,"column":3260}},{"source":"style.css","name":null,"original":{"line":305,"column":8},"generated":{"line":1,"column":3261}},{"source":"style.css","name":null,"original":{"line":305,"column":24},"generated":{"line":1,"column":3276}},{"source":"style.css","name":null,"original":{"line":306,"column":8},"generated":{"line":1,"column":3277}},{"source":"style.css","name":null,"original":{"line":306,"column":21},"generated":{"line":1,"column":3289}},{"source":"style.css","name":null,"original":{"line":307,"column":8},"generated":{"line":1,"column":3290}},{"source":"style.css","name":null,"original":{"line":308,"column":4},"generated":{"line":1,"column":3300}},{"source":"style.css","name":null,"original":{"line":310,"column":0},"generated":{"line":1,"column":3301}}],"sources":{"style.css":":root {\n    /* Colors */\n    --color-main: #0494cb;\n    --color-second: #0494cb;\n    --color-neutral: #abacaf;\n    --color-alert: rgb(235, 51, 51);\n\n    /* Fonts */\n    --font-title: 'Asap Condensed', sans-serif;\n    --font-body: 'Raleway', sans-serif;\n\n    /* Font Sizes */\n    --root-size: 16px;\n    --h1-size: 3.2em;\n    --h2-size: 2.2em;\n    --h3-size: 21px;\n    --h4-size: 1.1em;\n    --subtitle-size: 0.8em;\n    --tiny-size: 0.6em;\n}\n\nhtml,\nbody,\nh1,\nh2,\nh3,\np {\n    margin: 0;\n    padding: 0;\n}\n\nhtml {\n    font-size: var(--root-size);\n    font-family: var(--font-body);\n    background-color: #ececec;\n}\n\nbody {\n    font-family: var(--font-body);\n}\n\nh1 {\n    font-family: var(--font-title);\n    font-size: var(--h1-size);\n}\n\nh2 {\n    font-family: var(--font-title);\n    font-size: var(--h2-size);\n}\n\n.list {\n    column-count: 2;\n    margin: 15px 0px;\n}\n\n.center {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n\nbutton {\n    padding: 10px;\n    margin: 20px 0px;\n    width: 50%;\n    font-family: var(--font-body);\n    font-size: var(--h3-size);\n    font-weight: 900;\n}\n\n\n/* COLUMNS */\n\n.columns {\n    display: grid;\n    grid-template-columns: 1fr 2.2fr;\n    column-gap: 15px;\n    margin: 0px;\n}\n\n.status {\n    border: 1px solid grey;\n    margin: 1%;\n    padding: 5%;\n}\n\n.row1 {\n    margin: 0px 0px 15px 0px;\n}\n\n.step-header {\n    padding: 20px;\n    margin: 0%;\n    line-height: 200%;\n    background-color: var(--color-main);\n    color: white;\n}\n\n.step-content {\n    padding: 20px;\n    margin: 0;\n    background-color: white;\n}\n\n/* FORM FIELDS */\n\ninput,\nselect {\n    font-family: var(--font-body);\n    font-size: 1rem;\n    margin: 5px 0px;\n    padding: 10px;\n    width: 95.5%;\n    font-weight: 500;\n}\n\nselect {\n    width: 100%;\n}\n\n.form-field {\n    padding: 15px 10px;\n    background-color: white;\n    font-weight: 600;\n}\n\n/* STATUS */\n\n.success {\n    color: var(--color-second);\n}\n\n.fail {\n    color: var(--color-alert);\n}\n\n.neutral {\n    color: grey;\n}\n\n.inactive {\n    background-color: var(--color-neutral);\n    color: white;\n    border: 3px solid var(--color-neutral);\n}\n\n.active {\n    background-color: var(--color-main);\n    color: white;\n    border: 3px solid var(--color-main);\n    font-size: var(--h3-size);\n}\n\n.active:hover {\n    background-color: white;\n    color: var(--color-main);\n    border: 3px solid var(--color-main);\n    cursor: pointer;\n}\n\n.waiting {\n    margin: 0px;\n    padding: 15px;\n    background-color: white;\n    color: rgb(77, 77, 77);\n    font-size: var(--h4-size);\n    border: none;\n}\n\n.waiting:hover {\n    background-color: lightgrey;\n    cursor: pointer;\n}\n\n.hide {\n    position: absolute !important;\n    top: -9999px !important;\n    left: -9999px !important;\n}\n\n\n/* VALIDATION STEPS */\n\n.validation-step {\n    display: grid;\n    grid-template-columns: 5fr 1fr;\n    column-gap: 10px;\n    border-bottom: 1px solid lightgrey;\n}\n\n.validation-name {\n    font-size: var(--h3-size);\n    font-weight: 500;\n    margin: 0%;\n    padding: 15px 10px;\n}\n\n.validation-name:hover {\n    background-color: rgb(233, 232, 232);\n    cursor: pointer;\n}\n\n.validation-info {\n    margin: 0%;\n    padding: 2%;\n}\n\n.validation-symbol {\n    font-size: var(--h3-size);\n    font-weight: 900;\n}\n\n.toggle {\n    font-size: var(--tiny-size);\n    padding: 10px 0px 0px 0px;\n    color: var(--color-main);\n    font-weight: 600;\n}\n\n.info {\n    background-color: rgb(231, 231, 231);\n    color: rgb(51, 51, 51);\n    padding: 10px;\n    margin: 0px 0px 10px 0px;\n}\n\n.result {\n    padding: 10px;\n    margin: 10px 0px;\n}\n\n.helptext {\n    margin: 5px;\n    font-size: var(--subtitle-size);\n    font-weight: 600;\n    color: var(--color-alert);\n    text-align: center;\n    display: grid;\n    grid-template-columns: 1fr;\n}\n\n.asterisk {\n    color: var(--color-alert);\n    font-size: var(--h4-size);\n    font-weight: 900;\n}\n\n\n/* AGGREGATION */\n\n.reporting-month {\n    margin: 15px 0px 5px 0px;\n    font-size: var(--h3-size);\n    font-weight: 600;\n    color: var(--color-main)\n}\n\n.reporting-community {\n    margin: 5px 0px 15px 0px;\n    font-size: var(--root-size);\n}\n\n\n.agg-table {\n    display: grid;\n    grid-template-columns: 1fr 2fr 1fr 1fr;\n    column-gap: 0px;\n}\n\n.agg-header {\n    font-size: var(--h4-size);\n    font-weight: 600;\n    margin: 0px 0px;\n    padding: 6px;\n    background-color: lightgrey;\n}\n\n.agg-value {\n    font-size: var(--root-size);\n    margin: 0px 0px;\n    padding: 6px;\n    border-bottom: 1px solid lightgrey;\n}\n\n.agg-value:hover {\n    background-color: rgb(235, 235, 235);\n}\n\n.agg-spacer {\n    font-size: var(--root-size);\n    margin: 0px 0px;\n    padding: 6px;\n}\n\n\n\n/* MEDIA BREAKPOINTS */\n\n/* Smartphones (portrait) ----------- */\n@media only screen and (max-width : 800px) {\n    .columns {\n        display: grid;\n        grid-template-columns: 1fr;\n        column-gap: 10px;\n        row-gap: 20px;\n        margin: 5px;\n    }\n\n}"},"lineCount":null}},"error":null,"hash":"357ff551eb20855f92be4b2401f7011a","cacheData":{"env":{}}}