ap: 50px;\n    margin: 0px;\n}\n\n.status {\n    border: 1px solid grey;\n    margin: 1%;\n    padding: 5%;\n}\n\n.step-header {\n    padding: 20px;\n    margin: 0%;\n    line-height: 200%;\n    background-color: var(--color-main);\n    color: white;\n}\n\n.step-content {\n    padding: 20px;\n    margin: 0;\n    background-color: white;\n}\n\n/* FORM FIELDS */\n\ninput,\nselect {\n    font-family: var(--font-body);\n    font-size: 1rem;\n    margin: 5px 0px;\n    padding: 10px;\n    width: 95.5%;\n    font-weight: 500;\n}\n\nselect {\n    width: 100%;\n}\n\n.form-field {\n    padding: 15px 10px;\n    background-color: white;\n    font-weight: 600;\n}\n\n/* STATUS */\n\n.success {\n    color: var(--color-second);\n}\n\n.fail {\n    color: var(--color-alert);\n}\n\n.neutral {\n    color: grey;\n}\n\n.inactive {\n    background-color: var(--color-neutral);\n    color: white;\n    border: 3px solid var(--color-neutral);\n}\n\n.active {\n    background-color: var(--color-main);\n    color: white;\n    border: 3px solid var(--color-main);\n    font-size: var(--h3-size);\n}\n\n.active:hover {\n    background-color: white;\n    color: var(--color-main);\n    border: 3px solid var(--color-main);\n    cursor: pointer;\n}\n\n.waiting {\n    margin: 0px;\n    padding: 15px;\n    background-color: white;\n    color: rgb(77, 77, 77);\n    font-size: var(--h4-size);\n    border: none;\n}\n\n.waiting:hover {\n    background-color: lightgrey;\n    cursor: pointer;\n}\n\n.hide {\n    position: absolute !important;\n    top: -9999px !important;\n    left: -9999px !important;\n}\n\n\n/* VALIDATION STEPS */\n\n.validation-step {\n    display: grid;\n    grid-template-columns: 5fr 1fr;\n    column-gap: 10px;\n    border-bottom: 1px solid lightgrey;\n}\n\n.validation-name {\n    font-size: var(--h3-size);\n    font-weight: 500;\n    margin: 0%;\n    padding: 15px 10px;\n}\n\n.validation-name:hover {\n    background-color: rgb(233, 232, 232);\n    cursor: pointer;\n}\n\n.validation-info {\n    margin: 0%;\n    padding: 2%;\n}\n\n.validation-symbol {\n    font-size: var(--h3-size);\n    font-weight: 900;\n}\n\n.toggle {\n    font-size: var(--tiny-size);\n    padding: 10px 0px 0px 0px;\n    color: var(--color-main);\n    font-weight: 600;\n}\n\n.info {\n    background-color: rgb(231, 231, 231);\n    color: rgb(51, 51, 51);\n    padding: 10px;\n    margin: 0px 0px 10px 0px;\n}\n\n.result {\n    padding: 10px;\n    margin: 10px 0px;\n}\n\n.helptext {\n    margin: 5px;\n    font-size: var(--subtitle-size);\n    font-weight: 600;\n    color: var(--color-alert);\n    text-align: center;\n    display: grid;\n    grid-template-columns: 1fr;\n}\n\n.asterisk {\n    color: var(--color-alert);\n    font-size: var(--h4-size);\n    font-weight: 900;\n}\n\n\n/* AGGREGATION */\n\n.reporting-month {\n    margin: 15px 0px 5px 0px;\n    font-size: var(--h3-size);\n    font-weight: 600;\n    color: var(--color-main)\n}\n\n.reporting-community {\n    margin: 5px 0px 15px 0px;\n    font-size: var(--root-size);\n}\n\n\n.agg-table {\n    display: grid;\n    grid-template-columns: 1fr 2fr 1fr 1fr;\n    column-gap: 0px;\n}\n\n.agg-header {\n    font-size: var(--h4-size);\n    font-weight: 600;\n    margin: 0px 0px;\n    padding: 6px;\n    background-color: lightgrey;\n}\n\n.agg-value {\n    font-size: var(--root-size);\n    margin: 0px 0px;\n    padding: 6px;\n    border-bottom: 1px solid lightgrey;\n}\n\n.agg-value:hover {\n    background-color: rgb(235, 235, 235);\n}\n\n.agg-spacer {\n    font-size: var(--root-size);\n    margin: 0px 0px;\n    padding: 6px;\n}\n\n\n\n/* MEDIA BREAKPOINTS */\n\n/* Smartphones (portrait) ----------- */\n@media only screen and (max-width : 800px) {\n    .columns {\n        display: grid;\n        grid-template-columns: 1fr;\n        column-gap: 10px;\n        row-gap: 20px;\n        margin: 5px;\n    }\n}"},"lineCount":null}},"error":null,"hash":"d626c87eefa66adc197ad9f5a7c5a91a","cacheData":{"env":{}}}